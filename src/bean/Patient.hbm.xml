<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"'http://hibernate.sourceforge.net/hibernate-mappgin-3.0.dtd">
	
<hibernate-mapping>
	<class name="bean.Medecin" table="MEDECIN">
		<id name="id" column="codepat">
			<generator class = "increment"></generator>
		</id>
		
		<property name="Nom" column="Nom"></property>
		<property name="Prenom" column="Prenom"></property>
		<property name="Sexe" column="Sexe"></property>
		<property name="Adresse" column="Adresse"></property>
	</class>
</hibernate-mapping>